     <!-- Section: contact -->
     <section id="contact" class="home-section text-center">
         <div class="heading-contact">
             <div class="container-fluid">
             <div class="row">
                 <div class="col-lg-8 col-lg-offset-2">
                     <div class="wow bounceInDown" data-wow-delay="0.4s">
                     <div class="section-heading">
                     <h2>Let's keep in touch</h2>
                     <div class="lead">We'll keep you informed of new cities and data sources as we add them to the service.</div>
                     <i class="fa fa-2x fa-angle-down"></i>

                     </div>
                     </div>
                 </div>
             </div>
             </div>
         </div>
         <div class="container-fluid">
             <div class="row">
                 <div class="col-lg-12">
                     <div class="boxed-white">
                         <form id="contact-form">
                         <div class="row">
                             <div class="col-md-12">
                                 <div class="form-control-wrapper">
                                     <input class="form-control empty" type="email">
                                     <div class="floating-label">Your email address</div>
                                     <span class="material-input"></span>
                                 </div>
                             </div>
                             <div class="col-md-12">
                                 <button type="submit" class="btn btn-primary pull-right" id="btnContactUs">Stay up to date</button>
                             </div>
                         </div>
                         </form>
                     </div>
                 </div>
             </div>
         </div>
     </section>
     <!-- /Section: contact -->

